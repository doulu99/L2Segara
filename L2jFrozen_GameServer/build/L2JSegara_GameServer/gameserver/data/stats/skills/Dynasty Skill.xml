  <set name="skillType" val="BUFF"/>
  <set name="operateType" val="OP_PASSIVE"/>
  <set name="castRange" val="-1"/>
  <set name="effectRange" val="-1"/>
  <for>
   <add order="0x10" stat="STR" val="3" /> <!-- STR +3 -->
			<sub order="0x10" stat="CON" val="2" /> <!-- CON -2 -->
